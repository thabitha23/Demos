
class Emp{
	
	private int empid;
	private String empName;
	public int getEmpid() {
		System.out.println("Someone is trying to fetch the EmpId");
		return empid;
	}
	public void setEmpid(int empid) {
		this.empid = empid;
		System.out.println("Someone is trying to update the EmpId");

	}
	public String getEmpName() {
		System.out.println("Someone is trying fetch the EmpName");
		return empName;
	}
	public void setEmpName(String empName) {
		this.empName = empName;
		System.out.println("Someone is trying to update the EmpName");

	}
   
}


public class EncapsulationDemo {

	public static void main(String[] args) {
		 
		Emp obj = new Emp();
		obj.setEmpid(15);
		obj.setEmpName("Tabitha");
		obj.getEmpid();
		obj.getEmpName();
		
		
	}

}
