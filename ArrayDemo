
public class ArrayDemo {

	public static void main(String[] args) {
 
		int ary[][] = {
				{1, 2, 3, 4},
				{4 ,5,6},
				{7,8, 9, 1,2}
				 		
	};
		//Enhanced for loop or For-each loop
		for ( int k[] : ary) {
			for (int l: k) {
				System.out.print(" " +l);
			}
			System.out.println();
		}

}
}
